{
  "name": "generator-hw",
  "version": "0.0.1",
  "description": "Yeoman generator that provides templates for using nodejs",
  "files": [
    "generators"
  ],
  "scripts": {
    "postinstall": "postinstall-build generators/app/index.js",
    "clean": "npm run clean:build && npm run clean:dist",
    "clean:dist": "rimraf dist/**",
    "clean:build:lib": "rimraf generators/**",
    "clean:build": "rimraf build/**",
    "build": "npm run build:lib",
    "build:lib": "npm run clean:build:lib && tsc -p src/lib",
    "prepublishOnly": "npm run build",
    "lint": "tslint -p .",
    "check": "npm run clean && npm run build && npm run lint"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.108",
    "@types/yeoman-generator": "^2.0.3",
    "chalk": "^2.4.1",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.10",
    "postinstall-build": "^5.0.1",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chai-as-promised": "^7.1.0",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.8",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^4.3.3",
    "@types/sinon-chai": "^2.7.31",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.1.1",
    "nyc": "^11.7.3",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "source-map-support": "^0.5.0",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^15.0.0",
    "webpack": "^4.8.3",
    "webpack-dev-server": "^3.1.4"
  }
}
